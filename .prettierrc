{
    "singleAttributePerLine": false,
    "singleQuote": false,
    "jsxSingleQuote": false,
    "tabWidth": 4,
    "arrowParens": "always",
    "printWidth": 80,
    "semi": true,
    "trailingComma": "es5",
    "useTabs": false,
    "endOfLine": "lf",
    "bracketSameLine": false,
    "bracketSpacing": true,
    "plugins": ["prettier-plugin-tailwindcss"],
    "pluginSearchDirs": false
}
